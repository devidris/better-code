{
  "name": "better-code",
  "displayName": "better code",
  "description": "Help optimize and comment code",
  "publisher": "dev-idris",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "better-code.optimize",
        "title": "Optimize Code"
      },
      {
        "command": "better-code.comment",
        "title": "Comment Code"
      },
      {
        "command": "better-code.optimizeandcomment",
        "title": "Optimize and Comment Code"
      },
      {
        "command": "better-code.write",
        "title": "Write Code"
      },
      {
        "command": "better-code.debug",
        "title": "Debug and Fix Code"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.1.4",
    "openai": "^3.2.1"
  }
}
